// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// 3
model Link {
  id          Int      @id @default(autoincrement())   // 4
  createdAt   DateTime @default(now())  // 5
  description String
  url         String
}